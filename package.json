{
  "name": "hello-world",
  "version": "0.0.0-development",
  "description": "demo project for sr-cli",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/lib-rrio.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run util:clean:build && npm run util:rollup -c",
    "util:clean:build": "rm -rf build/",
    "util:clean:dist": "rm -r dist/*",
    "util:rollup": "rollup -c",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "jasmine"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmartin11/hello-world.git"
  },
  "keywords": [
    "hello"
  ],
  "author": "wmartin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wmartin11/hello-world/issues"
  },
  "homepage": "https://github.com/wmartin11/hello-world#readme",
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "ghooks": "^2.0.4",
    "jasmine": "^3.4.0",
    "rollup": "^1.8.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "semantic-release": "^15.13.3"
  },
  "dependencies": {
    "inquirer": "^6.2.2",
    "rxjs": "^6.4.0"
  },
  "czConfig": {
    "path": "node_modules/cs-conventional-change"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
